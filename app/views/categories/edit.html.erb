<%=render 'layouts/nav_bar'%>
<h1>Edit this category</h1>

<%= form_for @category do |f| %>

    <div class="form-group">
        <%= f.label :name %>
        <%= f.text_field :name %>
    </div>

    <div class="form-group">
        <%= f.label :description %>
        <%= f.text_field :description %>
    </div>
	<div class="form-group">
    <%= f.submit "Submit these changes!" %>
	</div>
<% end %>

